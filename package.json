{
  "name": "webpack-esmodule-example",
  "description": "Using webpack for both modern and legacy browsers, at the same time.",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "serve target/production",
    "prebuild": "run-p clean lint",
    "clean": "rimraf target",
    "lint": "modern lint",
    "build": "webpack --config=webpack.config.prod.js",
    "postbuild": "merge-html target/production/index-modern.html target/production/index-legacy.html > target/production/index.html && rimraf target/production/index-modern.html target/production/index-legacy.html",
    "build:modern": "webpack --config=webpack.config.prod.js --config-name=modern",
    "build:legacy": "webpack --config=webpack.config.prod.js --config-name=legacy",
    "dev": "run-p dev:*",
    "dev:modern": "webpack serve --config=webpack.config.dev.js --config-name=modern",
    "dev:legacy": "webpack serve --config=webpack.config.dev.js --config-name=legacy",
    "precommit": "modern precommit"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@hugojosefson/merge-html": "^3.0.0",
    "babel-loader": "^8.2.2",
    "babel-preset-solid": "^0.23.8",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.4",
    "modern-node": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "core-js": "3.8.3",
    "lit-dom-expressions": "^0.24.7",
    "regenerator-runtime": "^0.13.5",
    "serve": "^11.3.2",
    "solid-js": "^0.23.11"
  },
  "browserslist": {
    "legacy": [
      "ie 10",
      "firefox 40"
    ],
    "modern": [
      "chrome 61",
      "firefox 60",
      "edge 16",
      "safari 10.1"
    ]
  },
  "engines": {
    "node": ">=14.4"
  }
}
